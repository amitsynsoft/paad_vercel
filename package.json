{
  "name": "paad-ae",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "npm rebuild lightningcss || true",
    "dev": "next dev --turbopack",
    "dev:UAT": "cross-env NODE_ENV=test next dev --turbopack",
    "dev:PRODUCTION": "cross-env NODE_ENV=production next dev --turbopack",
    "build:DEVELOPMENT": "cross-env NODE_ENV=development next build",
    "build:UAT": "cross-env NODE_ENV=test next build",
    "build": "next build",
    "postbuild:DEVELOPMENT": "npm run copy-standalone-assets",
    "postbuild:UAT": "npm run copy-standalone-assets",
    "postbuild": "npm run copy-standalone-assets",
    "copy-standalone-assets": "shx mkdir -p .next/standalone/.next/static && shx cp -r .next/static/* .next/standalone/.next/static && shx mkdir -p .next/standalone/public && shx cp -r public/* .next/standalone/public",
    "start:without-standalone": "next start",
    "start": "node .next/standalone/server.js",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "dependencies": {
    "@heroui/react": "^2.8.4",
    "@radix-ui/react-direction": "^1.1.1",
    "@react-google-maps/api": "^2.20.7",
    "@reduxjs/toolkit": "^2.9.0",
    "cross-env": "^10.1.0",
    "framer-motion": "^12.23.22",
    "google-translate-api-x": "^10.7.2",
    "lucide-react": "^0.544.0",
    "moment": "^2.30.1",
    "next": "15.5.4",
    "next-intl": "^4.3.9",
    "prettier": "^3.6.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "rehype-raw": "^7.0.0",
    "swiper": "^12.0.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "shx": "^0.4.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
